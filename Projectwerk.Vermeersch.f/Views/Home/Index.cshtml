@model  IEnumerable<Projectwerk.Vermeersch.f.Models.Sport>

@{
    ViewBag.Title = "Home";
}

<div class="well well-sm" style="background:#D41024;color:white;">
    <h2>Home</h2>
</div>
<div style="height:10%; background-color:yellow;">
    <div id="temp">
        @Html.Raw(TempData["succesboodschap"])
    </div>
</div>

@if (!string.IsNullOrWhiteSpace(TempData["succesboodschap"] + ""))
{
    <script>
        $('#temp').slideDown();
    </script>
}
